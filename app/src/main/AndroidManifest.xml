<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yy.hao.com.testphonemvp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW"/>
    <application
        android:allowBackup="true"
        android:name="com.bao.tao.base.app.MyApp"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.bao.tao.base.view.PhoneActivity" />
        <activity android:name=".view.MainActivity"
            android:theme="@style/MainTheme"
            >
            <intent-filter>
                <action android:name="MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.bao.tao.base.view.design.CoordinatorLayoutActivity"

            />
        <activity android:name="com.bao.tao.base.view.design.CoordActivityOneActivity" />
        <activity android:name="com.bao.tao.base.view.design.CoordActivityThreeActivity" />
        <activity android:name="com.bao.tao.base.view.design.CoordActivityFourActivity" />
        <activity android:name="com.bao.tao.base.view.design.CoordActivityFiveActivity" />
        <activity android:name="com.bao.tao.base.view.design.TextInputActivity" />
        <activity android:name="com.bao.tao.base.view.NewsActivity" />
        <activity android:name="com.bao.tao.base.view.ShareElementActivity" />
        <activity android:name="hao.wen.zhang.guid.GuidActivity" />
        <activity android:name="hao.wen.zhang.chat.CharActivity" />
        <activity android:name="com.bao.tao.base.view.XuanFu" />

        <service android:name="com.bao.tao.base.view.XuanFuService" />
        <!--钟表-->

    </application>

</manifest>